#/bin/sh


rm msgapp.tar

version=latest
if [ $# -eq 1 ]
  then
    version=$1
fi

IMAGENAME=mycluster.icp:8500/kafka/msgapp:$version
#gradle build && gradle build war
docker build -t $IMAGENAME .
docker save -o msgapp.tar $IMAGENAME
scp msgapp.tar root@10.10.80.172:/root/mjkong/msgapp
#docker push mycluster.icp:8500/semi/sender:$version
